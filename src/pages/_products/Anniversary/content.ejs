<script id="PrizeTpl" type="text/template">
	{{if(!myModel.isLogin) { }}
	<div class='loginBtn'>
		<div class='login_btn'>
			<a href="/static/tpl/loginTpl.html" data-toggle="modal" data-target="#login_modal">SIGN IN</a>
		</div>
		<p class='info'>please log in first</p>
	</div>
	{{ } else{ }}
		{{ if(!myModel.succ) { }}
		<div class='noPrize'>
			<span>No record of prizes</span>
		</div>
		{{ } else { }}
		<div class='havePrize'>
			<div class='prizeBox'>
				<ul>
					{{_.each(myModel.data, function(val, key){ }}
						{{if(val.source == 1) { }}
							{{if(val.type == 1) { }}
							<li>
								<div class='img'>
									<img src="{{=val.listImg}}" alt="">
								</div>
								<div class='prize_Info'>
									<p class='prize clearfix'>
										<span class='pull-left'>{{= val.prizeName }}</span>
										<span class='pull-right'>{{= val.winTime}}</span>
									</p>
									<div class='info clearfix'>	
										<p>{{= val.prizeContent}}</p>
										{{if(val.recipientAddress != null) { }}
										<a href="javascript:;" data-index ={{=val.winningId}}  class='prizeInfo check Check'>Check</a>
										{{ }else { }}
										<a href="javascript:;" data-index ={{=val.winningId}} class='prizeInfo Redeem'>Redeem</a>
										{{ } }}
									</div>
								</div>
							</li>
							{{ }else { }}
							<li>
								<div class='img'>
									<img src="{{=val.listImg}}" alt="">
								</div>
								<div class='prize_Info'>
									<p class='prize clearfix'>
										<span class='pull-left'>{{= val.prizeName }}</span>
										<span class='pull-right'>{{= val.winTime}}</span>
									</p>
									<div class='info clearfix'>	
										<p>{{= val.prizeContent}}</p>
										{{if(val.category == 1) { }}
										<a href="/account/?src=myCoupon" data-index ={{=val.winningId}} class='prizeInfo Check'>Check</a>
										{{ }else { }}
										<a href="/account/?src=wallet" data-index ={{=val.winningId}}  class='prizeInfo Check'>Check</a>
										{{ } }}
										
									</div>
								</div>
							</li>
							{{ } }}
						
						{{ } else { }}
						<li>
							<div class='img'>
								<img src="{{=val.listImg}}" alt="">
							</div>
							<div class='prize_Info'>
								<p class='prize clearfix'>
									<span class='pull-left'>{{= val.prizeName }}</span>
									<span class='pull-right'>{{= val.winTime}}</span>
								</p>
								<div class='info noBtn'>	
									<p>{{= val.prizeContent}}</p>
								</div>
							</div>
						</li>
						{{ } }}
					
					{{ }) }}
				</ul>
			</div>
		</div>
		{{ } }}
	{{ } }}
</script>

