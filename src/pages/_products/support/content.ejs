<script id="carouselInfoTpl" type="text/template">
	{{if(myModel.length != 0) { }}
	<div class='tripInfo'>
		<div id="carousel" class="carousel slide" data-wrap = "false" data-interval = 'false'>
			<!-- Wrapper for slides -->
			<div id="carousel-inner">
				<div class="carousel-inner"  role="listbox">
				{{_.each(myModel, function(val,key) { }}
				<div class="item {{= key == 0 ? 'active' :''}}">
					<div class="carousel-caption">
						<div class="col-header clearfix">
							<p class="pull-left">Trip ID: <span class="">{{=val.flightItineraryIdString}}</span> <i class="icon icon-copy copyText"></i></p>
							<p class="pull-right">
								<span class="pull-left">Depart: {{=val.departTime_1}}</span>
								<span class="pull-right">{{=val.duration}} Travellers</span>
							</p>
							<div class='TipsMsg'>
								Successfully copied Trip ID to the clipboard
							</div>
						</div>
						<a href="/account/?src=trips/{{=val.flightItineraryIdString}}&token={{=val.token}}" target="_blank">
							<div class="col-flightInfo">
								<ul class="clearfix">
									<li class="font-Bold {{= val.bookStatus == 'Paid' ? 'paid' :''}}">{{=val.bookStatusText}}</li>
									<li>
										<img src="/static/images/air-logo/{{=(val.travellers).slice(0,2)}}.png" alt="airlogo"  class="logo"  onerror="this.src='/static/images/air-logo/default.png'">
										<p>{{=val.travellers}}</p>
									</li>
									<li>
										<p>{{=val.departTime_2}}</p>
										<p class="font-Bold">{{=val.departName}}</p>
									</li>
									<li>
										<p>{{=val.durationTime}}</p>
										<p></p>
										<p>{{= val.stop == 0 ? 'Non' :  val.stop}} - Stop</p>
									</li>
									<li>
										<p>{{= val.arriveTime_2}} <span  class="{{=val.isCrossDay? '' : 'hidden' }}"  style='color:#0b9d78;' >+1</span></p>
										<p class="font-Bold">{{=val.arriveName}}</p>
									</li>
								</ul>
							</div>
						</a>
						<div class="btnBox clearfix">
							<div class="flightBtn pull-left">
							  <a type='button' class="btn btn-default" target='_blank' href="/account/?src=trips/{{=val.flightItineraryIdString}}&token={{=val.token}}">View Details</a>
								<button type='button' {{=val.isChangeable ? "" : "disabled" }} target='_blank'  class='btn btn-default f-cancel {{=val.isChangeable ? "" : "disabled" }}' data-isoperateable="{{=val.isChangeable}}" tripid="{{=val.flightItineraryIdString}}" href="javascript:;">Cancel Flight</button>
								<button  type='button' href="javascript:;" target='_blank' class='btn btn-default f-change {{=val.isChangeable ? "" : "disabled" }}' {{=val.isChangeable ? "" : "disabled" }}  tripid="{{=val.flightItineraryIdString}}" data-isoperateable="{{=val.isChangeable}}">Change Flight</button>
							</div>
							<div class="printBtn pull-right">
								<button type='button' class="btn btn-default {{=( val.isPayable || !val.isTicketNo) ? 'disabled' : '' }}" {{=( val.isPayable || !val.isTicketNo) ? "disabled" : '' }} onclick="if({{= (val.isPayable || !val.isTicketNo)}}){return false}else{ location.href = '/heg_api/itinerary/printItinerary.do?tripId={{= val.flightItineraryIdString}}'}">Print Ticket</button>
								<button type='button' class="btn btn-default {{=val.status !=4 ? 'disabled' : ''}}" {{=val.status !=4 ? "disabled" : ''}} onclick="if({{= val.status != 4}}){return false}else{ location.href = '/heg_api/itinerary/printInvoice.do?tripId={{=val.flightItineraryIdString}}&mob={{=val.contactMob}}' }">Print invoice</button>
								<button type='button' class="btn btn-default {{=( val.isPayable || !val.isTicketNo) ? 'disabled' : '' }}" {{=( val.isPayable || !val.isTicketNo) ? "disabled" : ''}} id="emailTrip" href="javascript:;" tripid="{{=val.flightItineraryIdString}}">Email Itinerary</button>
							</div>
						</div>
						
					</div>
				</div>
				{{ }) }}
				</div>
			</div>
			<!-- Controls -->
			{{if(myModel.length != 1) { }}
			<a class="left carousel-control" href="#carousel" role="button" data-slide="prev"></a>
			<a class="right carousel-control" href="#carousel" role="button" data-slide="next"></a>
			{{ } }}
		</div>
	</div>
	{{ } else { }}
	<p class="title_1 ">No record found.Please check the information you entered.</p>
	{{ }  }}
	
	
</script>

