<script id="questionContTpl" type="text/template">
	<div class="panel-group tablist-group pull-left" id="accordion" role="tablist">
		{{_.each(myModel, function(val,key) { }}
		<div class="panel panel-default">
			<div class="panel-heading" role="tab" id="{{= 'collapseListGroupHeading_' + key}}">
				<h4 class="panel-title">
					<a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="{{= '#collapseListGroup' + key}}" aria-expanded="false" aria-controls="collapseListGroup1">
						{{= val.class}}
					</a>
				</h4>
			</div>
			<div id="{{= 'collapseListGroup' + key}}" class="panel-collapse collapse {{= key == 0 ? 'in' :''}}" role="tabpanel" aria-labelledby="{{= 'collapseListGroupHeading_' + key}}">
					<!-- Nav tabs -->
				<ul class="nav list-group" role="tablist">
					{{ _.each(val.node, function(value,index) {  }}
					<li role="presentation" class="list-group-item {{= (key == 0 && index == 0) ? 'active' : ''}}"><a href="{{= '#' + val.class.slice(0,3) +index}}" aria-controls="{{= val.class.slice(0,3) +index}}" role="tab" data-toggle="tab">{{= value.class}}</a></li>
					{{ })}}
				</ul>
			</div>
		</div>
		{{ })}}
	</div>
		<!-- Tab panes -->
	<div class="tab-content pull-right">
		{{_.each(myModel, function(val,key) { }}
			{{ _.each(val.node, function(value,index) {  }}
				<div role="tabpanel" class="tab-pane {{= (key == 0 && index == 0) ? 'active' : ''}}" id="{{= val.class.slice(0,3) +index}}">
				
					<div class="panel-group" id="accordion_{{=val.class.slice(0,3) + index}}" role="tablist" aria-multiselectable="true">
						{{ _.each(value.node, function(item,idx) {  }}
						<div class="panel panel-default">
							<div class="panel-heading" role="tab" id="{{='heading'+ val.class.slice(0,3) +index + idx}}">
								<h4 class="panel-title">
									<a class="font-Bold" role="button" data-toggle="collapse" data-parent="#accordion_{{=val.class.slice(0,3) + index}}" href="{{= '#collapse'+ val.class.slice(0,3) +index + idx}}" aria-expanded="true" aria-controls="{{= '#collapse'+ val.class.slice(0,3) +index + idx}}">
										{{= item.class}}
									</a>
								</h4>
							</div>
							<div id="{{= 'collapse' + val.class.slice(0,3) +index + idx}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="{{='heading'+ val.class.slice(0,3) +index  + idx}}">
								<div class="panel-body">
									<p>{{= item.content[0].content}}</p>
									<div class='fareBtn'>
										<a href="javascript:;" data-index= '1' data-id= '{{=item.cid}}' class=""><i class="icon icon_helpful"></i> Helpful</a>
										<a href="javascript:;" data-index= '0' data-id= '{{=item.cid}}'><i class="icon icon_notHelpful"></i> Not Helpful</a>
									</div>
								</div>
							</div>
						</div>
						{{ })}}
					</div>			
				</div>
			{{ })}}
		{{ })}}
	</div>
</script>


