}

.hotelOrderInfo {
  padding: 20px 30px;
}

.hotelDetail {
  .borderB {
    border-bottom: 1px solid #cdcdcd;
  }

  .pad8 {
    padding: 8px 0;
  }

  .statusBtn {
    position: absolute;
    right: 20px;
    top: 20px;
    width: 100px;
    height: 24px;
    color: #fff;
    line-height: 24px;
    text-align: center;
    background-color: @brand-primary;

