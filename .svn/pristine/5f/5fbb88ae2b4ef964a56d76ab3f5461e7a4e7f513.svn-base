@ggcolor: #cde7e0;

body {
  background-color: @gray-lighter;
  position: relative;
}

.fontSz14 {
  font-size: 14px;
}

.fontSz16 {
  font-size: 16px;
}

.fontB {
  font-weight: bold;
}

.widthP50 {
  width: 50%;
}

.padT20 {
  padding-top: 20px;
}

.padB20 {
  padding-bottom: 20px;
}

.popover {
  background-color: #000;
  opacity: .8 !important;
  color: #fff;

  .arrow:after {
    border-bottom-color: #000 !important;
    opacity: .8;
  }

  .dashBorder {
    padding-top: 8px;
    border-top: 1px dashed #ddd;
  }
}

.account-wrap {
  .container {
    width: 1200px;
    padding-top: 20px;
  }

  .aside {
    float: left;
    width: 286px;
    background-color: #fff;
    margin-right: 22px;
  }

  .primaryNav {
    dt {
      line-height: 26px;
      font-size: 14px;
      padding: 7px 30px;
      // margin-bottom: 30px;
      border-bottom: 1px solid @gray-lighter;

      &:before {
        content: ' ';
        float: left;
        width: 26px;
        height: 26px;
        margin-right: 20px;
        background-image: url(./imgs/icon-classification.png);
      }
    }

    dd {
      padding: 7px 30px;
      height: 40px;

      &.active {
        background-color: @brand-warning;

        i.icon.trips {
          background-image: url(./imgs/icon-trip2.png);
        }

        i.icon.hotels {
          background-image: url(./imgs/icon-hotel2.png)
        }

        i.icon.profile {
          background-image: url(./imgs/icon-profile2.png);
        }

        i.icon.traveller {
          background-image: url(./imgs/icon-traveler2.png);
        }

        i.icon.wallet {
          background-image: url(./imgs/icon-happyeasycash2.png);
        }

        i.icon.coupon {
          background-image: url(./imgs/icon-coupon2.png);
        }

        i.icon.print {
          background-image: url(./imgs/icon-printticket2.png);
        }

        i.icon.cancel {
          background-image: url(./imgs/icon-cancellation2.png);
        }

        i.icon.security {
          background-image: url(./imgs/icon-security2.png);
        }
      }

      a {
        display: block;
        line-height: 26px;
        font-size: 14px;
      }
    }

    i.icon {
      width: 25px;
      height: 25px;
      float: left;
      background-repeat: no-repeat;
      margin-right: 20px;
    }

    i.icon.trips {
      background-image: url(./imgs/icon-trip1.png);
    }

    i.icon.hotels {
      background-image: url(./imgs/icon-hotel1.png)
    }

    i.icon.profile {
      background-image: url(./imgs/icon-profile1.png);
    }

    i.icon.traveller {
      background-image: url(./imgs/icon-traveler1.png);
    }

    i.icon.wallet {
      background-image: url(./imgs/icon-happyeasycash1.png);
    }

    i.icon.coupon {
      background-image: url(./imgs/icon-coupon1.png);
    }

    i.icon.print {
      background-image: url(./imgs/icon-printticket1.png);
    }

    i.icon.cancel {
      background-image: url(./imgs/icon-cancellation1.png);
    }

    i.icon.security {
      background-image: url(./imgs/icon-security1.png);
    }
  }

  .rightside,
  .s-common {
    min-height: 300px;
    float: left;
    width: 872px;

    h2.title {
      height: 40px;
      padding: 14px 10px;
      font-size: 14px;
      color: @brand-primary;
      border-bottom: 1px solid @gray-lighter;
      background-color: #f8f8f8;
    }

    overflow: hidden;

    .content {
      background-color: #fff;
    }
  }

  .unLogin-wrap {
    .formField {
      background-color: #fff;
      padding: 40px 20px 30px;

      .notice {
        font-size: 16px;
        color: @brand-warning;
        line-height: 30px;
      }

      .form-control {
        width: 240px;
        padding-top: 0;
        padding-bottom: 0;
      }
    }
  }

  .s_loading {
    .progress {
      margin: 0 auto;
      border-radius: 12px;
    }

    .title {
      margin: 40px auto;
      text-align: center;
      font-size: 26px;
    }
  }

  .no-login,
  .f-detail,
  .s_loading {
    width: 872px;

    .message {
      margin: 60px auto;
      text-align: center;

      p {
        font-size: 20px;
        line-height: 32px;
      }

      .marT50 {
        margin-top: 50px;
      }

      .btn-custom {
        background-color: @brand-warning;
      }
    }
  }
}

.tripsTitle {
  font-size: 18px;
  border-bottom: 1px solid @gray-lighter;
  background-color: #f8f8f8;
  padding: 0 20px;

  ul {
    li {
      float: left;
      height: 45px;
      width: 90px;
      line-height: 45px;
      margin-right: 30px;

      a {
        display: inline-block;
        height: 45px;

        &:hover,
        &:focus {
          color: #000;
        }
      }

      &:first-child {
        width: 120px;

        a {
          position: relative;

          i {
            position: absolute;
            right: -32px;
            top: 8px;
            width: 30px;
            height: 20px;
            line-height: 20px;
            text-align: center;
            border-radius: 20px;
            background: red;
            color: white;
            font-size: 14px;
          }
        }
      }

      &:last-child {
        a {
          color: #999;

          &:hover,
          &:focus {
            color: #999;
          }
        }
      }
    }

    .active {
      a {
        color: #0b9d78;
        font-weight: 700;
        border-bottom: 3px solid #0b9d78;

        &:hover,
        &:focus {
          color: #0b9d78;
        }
      }
    }
  }
}

.noList {
  padding: 51.5px 0;
  text-align: center;
  background-color: #fff;

  p {
    line-height: 18px;
    font-size: 14px;
  }

  a {
    margin-top: 20px;
  }
}

.list-wrap {
  border-bottom: 1px solid @gray-light;

  .query-form {
    padding: 20px;

    .group {
      float: left;
      width: 160px;
      margin-right: 10px;

      label {
        float: left;
        padding: 0 10px 0 16px;
        line-height: 26px;
        font-weight: normal;
        padding-right: 8px;
        text-align: right;
      }

      .trip-query {
        float: left;
        width: 100%;
        border: none;
        border: 1px solid @gray-light;
        outline: none;
        height: 35px;
        padding-left: 10px;

        input {
          width: 94%;
          border: none;
          outline: none;
          line-height: 33px;
        }

        select {
          width: 100%;
          line-height: 26px;
          height: 32px;
          border-radius: 4px;
          padding-left: 4px;
        }

        &.select {
          border: none;

          select {
            width: 80%;
          }
        }

        .icon {
          display: inline-block;
          width: 20px;
          height: 20px;
          background-image: url(./imgs/icon-search.png);
          cursor: pointer;
        }
      }

      button {
        width: 120px;
        height: 35px;
      }

      &:last-child {
        width: 120px;
        margin-left: 15px;
      }
    }

    .hotel-group {
      width: 50%;

      label {
        width: 118px;
      }

      button {
        width: 100%;
      }

      .checkDate {
        cursor: pointer;
      }

      .icon_datepick {
        position: absolute;
        display: inline-block;
        width: 16px;
        height: 15px;
        background-image: url(./imgs/calendar.png);
        background-repeat: no-repeat;
        top: 6px;
        right: 5px;
        cursor: pointer;
      }
    }
  }

  .tab-info {
    p {
      width: 100%;
      padding: 0px 20px;
      line-height: 40px;
      border-top: 1px solid #ccc;
      border-bottom: 1px solid #ccc;
      font-size: 16px;

      span {
        color: #0b9d78;
      }
    }
  }

  .noOrderList {
    margin: 100px auto 300px;
    width: 290px;
    height: 154px;
    background: url('./imgs/noList.png') no-repeat;
  }

  .loadTimeOut {
    background: url('./imgs/loadMore.png') no-repeat;
  }

  .orders {
    a.order-item {
      display: block;
      padding: 16px 20px;
      color: @gray-darker;
      border-top: 1px solid #ccc;
      cursor: pointer;

      &.Completed,
      &.Refunded,
      &.Timeout {
        .c-month {
          background-color: @gray  !important;
        }

        .status-btn {
          background-color: @gray  !important;
        }
      }

      &.Tobepaid {
        .status-btn {
          background-color: @brand-warning  !important;
        }

        .cancel-btn {
          width: 120px;
          height: 30px;
          background: #ECECEC;
          border-radius: 4px;
          line-height: 30px;
          text-align: center;
          margin-top: 10px;
          color: #999999;
          z-index: 10;
        }
      }

      &:first-child {
        border-top: none;
      }

      &:hover {
        background-color: lighten(desaturate(@brand-primary, 30%), 56%);
      }

      li {
        float: left;

        &.fore0 {
          width: 27%;
          padding: 14px 90px 10px 30px;
        }

        &.fore1 {
          width: 46%;
          padding: 20px 0;
          border-left: 1px dashed #ccc;
          border-right: 1px dashed #ccc;
        }

        &.fore2 {
          width: 27%;
          padding: 14px 0 0 70px;
        }

        .icHotel {
          width: 37px;
          height: 31px;
          background: url(./imgs/icon-calendar-normal.png) no-repeat;
        }
      }

      .card {
        width: 86px;
        height: 90px;
        text-align: center;
        border-radius: 10px;
        overflow: hidden;

        .c-month {
          background-color: @brand-danger;
          height: 26px;
          line-height: 26px;
          color: contrast(@gray-base);
          font-size: 10px;
        }

        .c-data {
          background: radial-gradient(circle at center, #fff, @ggcolor), @gray-lighter;
          height: 64px;
          padding-top: 10px;
        }

        .day {
          display: block;
          color: @gray-dark;
          font-size: 20px;
          font-weight: bold;
        }

        .week {
          display: block;
          height: 24px;
          line-height: 24px;
          color: @gray-light;
          font-size: 14px;
          font-weight: 500;
        }
      }

      .f-route {
        text-align: center;

        .citys {
          margin-bottom: 18px;
        }

        span.dc,
        span.ac {
          font-size: 16px;
          font-weight: bold;
          margin: 0 20px;
        }

        .passengers {
          padding: 0 20px;
          font-size: 14px;
          color: @gray-light;
        }
      }

      .other-info {
        .status-btn {
          width: 120px;
          height: 28px;
          text-align: center;
          line-height: 30px;
          background-color: @brand-primary;
          color: #fff;
          border-radius: 4px;
        }

        .info {
          margin: 10px 0;
          color: @gray-light;
        }

        .id,
        .data {
          font-size: 14px;
        }
      }
    }
  }
}

.f-detail {
  h3 {
    font-size: 14px;
  }

  .payWrap {
    padding: 10px 20px;

    .goldBox {

      padding: 10px 0;
    }

    .form-gro {
      // margin-bottom: 5px;
      height: 28px;

      input {
        border: 1px solid #ccc;
      }
    }

    .form-con {
      width: 100%;
      height: 28px;
      background-color: #fff;
      padding: 10px;

      &[readonly] {
        background-color: #eee;
      }
    }
  }

  .paySummary {
    .paySession {
      background-color: #ffefd8;
      margin-left: -40px;
      margin-right: -40px;
      text-align: center;
    }
  }

  .payTypes {
    .nav-tabs {
      border-bottom: none;

      &>li {
        margin-bottom: 0;
        height: 46px;
      }

      &>li>a {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        height: 100%;
        background-color: @gray-lighter;
        padding: 0 22px;
        border: none;
        min-width: 100px;
        // text-align: center;
      }

      &>li.active>a {
        color: #111;
        background-color: #fff;
        border: none;
      }

      &>li>a:hover {
        // background-color: @brand-primary;
        color: #111;
        border: none;
      }

      .pay-tab_label {
        font-size: 12px;
        padding: 0 4px;
        background-color: #E3A428;
        border-radius: 2px;
        color: #fff;
      }
    }

    .cardWrap {
      padding: 20px 0 30px;

      .cardGather {
        padding-bottom: 20px;

        i {
          float: right;
          width: 234px;
          height: 20px;
          background: url('./imgs/gather.png');
        }
      }

      input,
      select {
        width: 100%;
        height: 36px;
        padding: 0 10px;
      }
    }

    .bankWrap {
      padding: 20px 0 10px;
    }

    .bankList {
      label {
        width: 100%;
        border: 1px solid #999;
        padding: 8px 18px;
      }

      input[type=radio] {
        float: left;
      }

      .bankIcon {
        display: block;
        float: left;
        width: 120px;
        height: 20px;
        margin-left: 20px;
      }
    }

    .otherList {
      select {
        width: 100%;
        height: 38px;
        padding: 0 10px;
      }
    }

    .upi {
      padding: 20px 0 10px;

      input {
        height: 36px;
        width: 300px;
        padding-left: 10px;
      }
    }
  }
}

.common-style {
  background-color: #fff;
}

.hotelInfo {
  margin-bottom: 20px;

  ul {
    display: flex;

    li {
      // height: 200px;
      background: #F8FBFB;
      margin-right: 20px;
      flex: 1;

      &:last-child {
        margin-right: 0px;
      }

      a {
        display: inline-block;
        width: 100%;
        height: 100%;

        &:hover {
          color: #333333;
        }
      }

      img {
        width: 100%;
        // height: 200px;
      }

      .info {
        padding: 10px;

        p {
          &:nth-child(1) {
            font-size: 16px;
            line-height: 24px;
          }

          &:nth-child(2) {
            text-align: right;

            span {
              font-size: 20px;
              color: #DC2323;
              line-height: 30px;
              font-weight: 900;

              span {
                font-size: 16px;
              }
            }
          }
        }
      }
    }
  }
}

.returnPre {
  width: 100%;
  height: 120px;
  margin-bottom: 20px;
  background: url(./imgs/background.png) no-repeat;
  padding: 20px 10px 20px 20px;
  position: relative;

  .pull-left {
    p {
      font-size: 20px;
      color: #FFFFFF;
      line-height: 30px;
      font-weight: 700;

      span {
        color: #FDE742;
      }

      &:nth-child(3) {
        font-size: 12px;
        font-weight: 400;
      }
    }
  }

  .pull-right {
    width: 300px;
    text-align: center;
    position: absolute;
    right: 60px;
    top: 20px;

    .share {
      line-height: 25px;
      font-size: 16px;
      color: #FFFFFF;
      position: relative;
      margin-top: 5px;

      &::before {
        content: '';
        position: absolute;
        left: 50px;
        top: 13px;
        width: 40px;
        height: 0;
        border-bottom: 1px solid white;
      }

      &::after {
        content: '';
        position: absolute;
        right: 50px;
        top: 13px;
        width: 40px;
        height: 0;
        border-bottom: 1px solid white;
      }
    }

    ul {
      li {
        float: left;
        width: 33.3%;
        position: relative;

        img {
          width: 30px;
          height: 30px;
        }

        p {
          color: white;
          margin-top: 10px;
        }

        .copyText {
          position: absolute;
          left: 99999px;
          top: 0;
        }
      }
    }

    .TipsMsg {
      position: absolute;
      right: 30px;
      top: 105px;
      width: 180px;
      padding: 10px;
      font-size: 12px;
      color: white;
      background: rgba(0, 0, 0, 0.8);
      border-radius: 4px;
      display: none;
    }
  }
}

.flight-info {
  .f-detail {
    padding: 10px 0;

    h3.tripTil {
      padding-left: 20px;
    }
  }

  .f-info {
    padding: 10px 0px;

    .til {
      padding-left: 20px;
      font-size: 20px;
      font-weight: bold;
      margin-bottom: 8px;

      .time {
        font-size: 16px;
        font-weight: normal;
        color: @gray;
      }
    }

    .vendor,
    .start,
    .through,
    .end,
    .baggage {
      float: left;

      span,
      small {
        display: block;
      }
    }

    .vendor {
      width: 18%;
      padding-left: 50px;
    }

    .vendor,
    .start,
    .end {
      span {
        font-size: 16px;
      }

      small {
        color: @gray;
      }
    }

    .start {
      width: 26%;
      text-align: right;
    }

    .through {
      width: 10%;
      text-align: center;
      padding-top: 20px;
    }

    .end {
      width: 26%;
    }

    .layover {
      padding: 6px 0;

      .line {
        float: left;
        width: 34%;
        height: 10px;
        border-bottom: 1px solid #ccc;
      }

      .layoverTime {
        float: left;
        width: 20%;
        text-align: center;
      }
    }

    .baggage {
      margin-left: 10px;

      .icon {
        float: left;
        padding-top: 2px;
        margin-left: -12px;
      }

      h3 {
        font-weight: 700;
        line-height: 1.3;
      }
    }
  }
}

.services-info {
  .operateNav {
    width: 50%;
    padding: 26px 20px;

    a.operations {
      float: left;
      width: 118px;
      border: 1px solid @brand-primary;
      border-radius: 4px;
      padding: 6px 12px;
      font-size: 14px;
      text-align: center;
      margin-right: 12px;
      cursor: pointer;
      color: @brand-primary;

      &.active,
      &:hover {
        background-color: @brand-primary;
        color: #fff;
      }
