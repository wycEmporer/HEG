.hotelPolicy {
  ul.pol-info {

    li {
      height: 46px;
      line-height: 46px;
      margin-right: 30px;
    }

    .icon-box {
      width: 46px;
      height: 46px;
      margin-right: 10px;
      background-repeat: no-repeat;
      background-position: center;

      &.icon-clock {
