      width: 100%;
      height: 362px;
      // background: url(./imgs/ad.png);
      position: relative;
      margin: 25px 0 35px;
      .ad_img{
        width: 100%;
        height: 100%;
      }
      .load{
        width: 315px;
        height: 41px;
        position: absolute;
        left: 30px;
        bottom: 20px;
        a{
          display: inline-block;
          height: 41px;
          width: 142px;
        }
      }
    }
    .nextTrip{
      .discoverCard{
        ul{
          li{
            position: relative;
            margin:0 20px 20px 0;
            float: left;
            .link{
              width: 330px;
              height: 66px;
              text-align: left;
              p{
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
                span{
                  font-size: 12px;
                  color: #B4B4B4;
                }
                &:nth-child(1){
                  width: 80%;
                }
                &:nth-child(2){
                  width: 90%;
                }
              }
            }
            &:nth-child(3n+2){
              margin-right: 0px;
            }
            &:nth-child(3n+3){
              margin-right: 20px;
            }
            &:nth-child(1){
              width: 626px;
              img{
                width: 100%;
              }
              .link{
                width: 460px;
                height: 66px;
              }
            }
            &:nth-child(2){
              width: 444px;
              img{
                width: 100%;
              }
              .link{
                width: 424px;
                height: 66px;
              }
            }
          }
        }
      }
    }
  }
  .sendEmail{
    width: 100%;
    height: 140px;
    background: #F6F6F6;
    border: 1px solid #DCDCDC;
    border-left: none;
    border-right: none;
    color: #000;
    padding: 30px 0;
    .container{
      .pull-left{
        p{
          &:nth-child(1){
            font-size: 28px;
            line-height: 46px;
          }
          &:nth-child(2){
            font-size: 16px;
            line-height: 30px;
          }
        }
      }
      .form-group{
        height: 40px;
        width: 440px;
        font-size: 16px;
        margin-top: 30px;
        input{
          width: 300px;
          height: 40px;
          background: #FFFFFF;
          border: 1px solid #ccc;
          border-radius: 2px;
          color: #787878;
          padding: 6px 10px;
          &:focus{
            border: 1px solid #E3A428;
            box-shadow: none;
          }
        }
        a{
          display: inline-block;
          width: 140px;
          height: 40px;
          line-height: 28px;
          background: #E3A428;
          color: white;
          border-radius: 0px 2px 2px 0px;
        }
      }
    }
  }
  
 
}
// banner轮播图样式
.L-content{
  .container{
    padding:0px; 
    position: absolute;
    left: 50%;
    height: 42px;
    top: 50%;
    z-index: 1;
    transform: translate(-50% ,-50%);
  }
  h1.title{
    font-weight: bold;
    font-size:18px;
    padding-bottom: 10px;
    line-height: 18px;
    color: #333;
  }
 
  .banner-wrap{
    height: 420px;
    overflow: hidden;
    position: relative;
    &:hover{
      .carouselControl,
      .carousel-indicators{
        display: block;
      }
    }
    .carousel-indicators{
      display: none;
      text-align: right;
      z-index: 0;
      li{
        margin-right:5px; 
        width: 14px;
        height: 8px;
        opacity: 0.5;
        background: #FFFFFF;
        border-radius: 0;
        &.active{
          background: #017959;
          border-color: #017959;
          opacity: 1;
        }
      }
    }
    .loop-inner{
      // position: relative;
      // height:100%;
    }
    .item{
      width:100%;  
      // position:relative;  
      overflow:hidden;  
      height:420px;  
      img{
        height: 420px;
        width:1920px;  
        position:absolute;  
        left:50%;  
        margin-left:-960px;  
        max-width: none;
      }
    }
    .over23, .cur234{
      opacity: 1;
      visibility: visible;
    }
    .cur234{
      transition: opacity 1s ease-in;
      z-index: 2;
    }

    .carouselControl{
      display: none;
    }
    .carousel-control{
      width: 45px;
      height: 64px;
      background: rgba(0,0,0,.1);
      top: 50%;
      transform: translateY(-50%);
      padding: 16px 14px;
      span{
        width: 17px;
        height: 32px;
      }
    }
    .left{
      span{
        margin: -15px 0 0px -9px;
        background: url('./imgs/Path5.png') no-repeat;
      }
      &:hover{
       span{
        
        background: url('./imgs/Path5_h.png') no-repeat;
       }
      }
    }
    .right{
      span{
        margin: -15px -9px 0px 0px;
        background: url('./imgs/Path6.png') no-repeat;
      }
      &:hover{
        span{
          background: url('./imgs/Path6_h.png') no-repeat;
        }
      }
    }
  }
}

.autoComplete{
  &:empty{
    display: none;
  }
  max-height: 292px;
  overflow-y: auto;
  background-color:#fff;
