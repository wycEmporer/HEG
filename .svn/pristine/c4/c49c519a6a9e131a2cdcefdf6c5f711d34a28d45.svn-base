
@themeColor: #2ca5ea;
body{
	background: #F2E9DD;
	min-width: 1275px;
}
@media screen and (max-width:1500px){
	.add-search #searchForm{
		padding-left:0;
	}
}

.content{
	position: relative;
	height: 1400px;
	.add-search{
		padding:10px 0;
		background:rgba(11,157,120,0.3) ;
		z-index: 10;
		width: 100%;
		position: absolute;
		#searchForm{
			min-width: 1275px;
			position: relative;
			width: 1275px;
			height: 40px;
			margin:auto;
			.checkError{
				position:absolute;
				top:30px;
				margin-top: 0;
			}
			nav{
				float: left;
				margin-right:30px;
				color:#fff;
				label{
					margin-bottom: 0;
					font-size:12px;
					font-family: "HelveticaLTPro-Roman";
					color:#fff;
					input{
						margin:0 10px 0 0;
					}
					strong{
						vertical-align: top;
					}
				}
				li{
					height:14px;
				}
				li.oneway{
					margin-bottom:12px;
				}
			}
			.LS-trip{
				::-webkit-input-placeholder{
					color:#999;
					opacity:100%;
					font-size: 14px;
					font-family: "HelveticaLight";
				}
				position: absolute;
				top: 0;
				left: 100px;
				.img-icon{
					width: 20px;
					height: 14px;
					background:url('./images/plane-up.png') no-repeat 100% 100%;
					position: absolute;
					top: 8px;
					right: 10px;
					z-index: 1;
				}
				.landing{
					background:url('./images/plane-down.png') no-repeat 100% 100%;
				}
				.calendar{
					background:url('./images/calendar.png') no-repeat 100% 100%;
					cursor: pointer;
				}
				.arrows{
					width: 20px;
					height: 84%;
					background: #fff;
					top: 1px;
					p{
						width: 20px;
						height: 14px;
						background:url('./images/up-down-arrow.png') no-repeat 100% 100%;
						cursor: pointer;
						position: absolute;
						top: 25%;
					}
				}
				.section1{
					float: left;
					.form-group{
						float:left;
						margin:6px 30px 0 0;
						position: relative;
						input{
							width: 200px;
							height: 30px;
							line-height: 30px;
							padding:0 30px 0 10px;
							color:#999;
						}
					}
				}
				.section2{
					float: left;
					.form-group{
						float:left;
						margin:6px 30px 0 0;
						.time-input{
							width: 160px;
							padding:0 10px;
							height: 30px;
							line-height: 30px;
							font-size: 12px;
						}
					}
					.d-date, .r-date{
						// width:40%;
						margin-right:20px;
						position: relative;
						input{
							cursor: pointer;
							background-color:#fff;
						}
						}
						.datepick{
						position:absolute;
						top:5px;
						right:10px;
						cursor: pointer;
						opacity: 0;
						z-index: 2;
						}
						.d-date{
						.icon{
							display: inline-block;
							width:20px;
							height:18px;
							background-image: url(./images/dapartbg.png);
							background-repeat: no-repeat;
						}
						}
						.r-date{
						.icon{
							display: inline-block;
							width:20px;
							height:18px;
							background-image: url(./images/returnbg.png);
							background-repeat: no-repeat;
						}
					}
				}
				.section3{
					float:left;
					margin:6px 20px 0 0;
					.selectItem{
						float:left;
						position: relative;
						width: 160px;
						margin: 0;
						#pIcon{
							vertical-align: 3%;
						}
						a.trallerpop{
							display: block;
							width:100%;
							border-radius: 4px;
							color:rgb(85,85,85);
							border:1px solid rgb(204, 204, 204);
							height:30px;
							line-height: 27px;
							padding:0 10px 0 10px;
							background: white;
						}
						.SelectWrap{
							top:40px;
							left:0;
							padding:0 10px 10px;
							width: 333px;
							max-width: 333px;
							.fore{
								width:100%;
								position: relative;
								border-bottom:1px solid #eee;
								p{
									margin: 10px 0;
								}
								#A_passenger,#C_passenger,#I_passenger{
									border:none;
									outline: none;
									width: 78px;
									height: 30px;
									line-height: 30px;
									padding:0 0  0 30px;
									// font-weight: 500;
									// font-size: px;
									// font-family: ;
								}
								.img-type{
									display:inline-block;
									width: 18px;
									height: 21px;
									vertical-align: text-bottom;
								}
								.A-type{
									background:url('./images/adults.png') no-repeat 100% 100%;
								}
								.C-type{
									background:url('./images/children.png') no-repeat 100% 100%;
								}
								.I-type{
									background:url('./images/infants.png') no-repeat 100% 100%;
								}
								.minus-plus{
									width: 64px;
									height: 22px;
									position: absolute;
									top: 30px;
									right: 0;
									display:flex;
									justify-content: space-between;
									span{
										display:inline-block;
										width: 22px;
										height: 22px;
										background-size: 100% 100%;
										cursor: pointer;
									}
									.minus-img{
										background: url('./images/minus.png') no-repeat;
									}
									.plus-img{
										background: url('./images/plus.png') no-repeat;
									}
								}
							}
							.S-seat{
								padding:10px 0 0;
								.s-seatType{
									label{
										display:block;
										margin-bottom:0;
									}
									.seat-type{
										width: 14px;
										height: 14px;
									}
									strong{
										font-size: 12px;
										color:#666;
										vertical-align: 30%;
									}
								}
								.s-seatRegular{
									padding:10px 0 20px;
									p{
										font-size: 12px;
										color:#666;
										font-family: "HelveticaLTPro-Light";
										letter-spacing: 0px;
										line-height: 16px;
									}
								}
							}
							.B-confirm{
								input[type="button"]{
									display:block;
									margin:auto;
									width: 280px;
									height: 34px;
									line-height: 34px;
									text-align: center;
									color:#fff;
									background:#1B2137;
									border:none;
								}
							}
						}
					}
				}
				.section4{
					float: left;
					button{
						width: 172px;
						height: 30px;
						line-height: 30px;
						padding:0 0;
						color: #29343c;
						font-size: 14px;
						font-family: "HelveticaLTPro-Roman";
					}
				}
			}
		}
	}
	.banner{
		position: relative;
		width:100%;
		height: 570px;
		overflow: hidden;
		.banImg{
			position: absolute;
			left:50%;
			transform: translateX(-50%);
		}
	}
	.head{
		position: relative;
		.Countdown{
			position: absolute;
			left: 0;
			bottom: 0;
			width: 100%;
			height: 170px;
			background: rgba(0	,0	,0	, 0.5);
			.Countdown_time{
				width: 100%;
				height: 100%;
				color: white;
				text-align: center;
				padding-top: 15px;
				i{
					font-style: inherit;
				}
				.in,.end{
					font-size: 36px;
					height: 80px;
					span{
						font-size: 60px;
					}
				}
				.in{
					line-height: 80px;
				}
				.time{
					font-size: 20px;
				}
			}
		}
	}
	.wrap{
		width: 100%;
		padding: 20px 0px 0px;
		min-width: 1200px;
		position: absolute;
		left: 0;
		top: 530px;
		.container{
			padding: 0px;
			text-align: center;
			.sale_box{
				background: white;
			}
			.sale{
				width: 100%;
				height: 298px;
				margin-bottom: 20px;
			}
			.app_sale{
				position: relative;
				background: url(./images/bg.png) no-repeat;
				.sale_left{
					width: 600px;
					height: 100%;
					padding: 55px 93px 40px 90px;
					text-align: center;
					position: relative;
					.phoneImg{
						width: 118px;
						margin-top: 10px;
						p{
							font-size: 16px;
							color: #666666;
							margin-top: 20px;
						}
					}
					.downLoad{
						p{
							height: 28px;
							margin-bottom: 10px;
							&:nth-child(1){
								text-align: right;
								font-size: 20px;
								color: #000000;
								
								span{
									color: red;
									font-weight: 700;
								}
							}
							&:nth-child(2){
								font-size: 16px;
								color: #151515;
								.copyText{
									margin-top:7px;
								}
								.copyIcon{
									width: 28px;
									height: 28px;
									background: url(./images/copy2.png) no-repeat;
									margin-left: 20px;
								}
							}
							&:nth-child(4){
								font-size: 14px;
								color: #666666;
								letter-spacing: 0;
								text-align: right;
							}
						}
					}
					.code{
						width: 88px;
						height: 88px;
						margin:0px 0px 10px 19px;
						img{
							width: 100%;
						}
					}
					&::after{
						content: '';
						position: absolute;
						right: 0;
						top: 40px;
						width: 0;
						height: 220px;
						border-left: 1px dashed #CDCDCD;
					}
				}
				.sale_right{
					width: 600px;
					height: 100%;
					padding: 55px 40px 40px 40px;
					text-align: center;
					.phoneImg{
						width: 210px;
						margin-top: 45px;
						p{
							font-size: 16px;
							color: #666666;
							margin-top: 20px;
						}
					}
					.downLoad{
						p{
							height: 28px;
							margin-bottom: 10px;
							&:nth-child(1){
								text-align: right;
								font-size: 20px;
								color: #000000;
								span{
									color: red;
									font-weight: 700;
								}
							}
							&:nth-child(2){
								font-size: 16px;
								color: #151515;
								.copyText{
									margin-top:7px;
								}
								.copyIcon{
									width: 28px;
									height: 28px;
									background: url(./images/copy2.png) no-repeat;
									margin-left: 20px;
								}
							}
							&:nth-child(4){
								font-size: 14px;
								color: #666666;
								letter-spacing: 0;
								text-align: right;
							}
						}
					}
					.code{
						width: 88px;
						height: 88px;
						margin:0px 0px 10px 19px;
						img{
							width: 100%;
						}
					}
				}
				.TC{
					position: absolute;
					left: 50%;
					bottom: 10px;
					transform: translateX(-50%);
					font-size: 14px;
					color: #666666;
					&::before{
						content: '';
						position: absolute;
						left: -80px;
						top: 9px;
						width: 60px;
						height: 0;
						border-top: 1px solid #CDCDCD;
					}
					&::after{
						content: '';
						position: absolute;
						right: -80px;
						top: 9px;
						width: 60px;
						height: 0;
						border-top: 1px solid #CDCDCD;
					}
				}
				.TipsMsg{
					position: absolute;
					top: 130px;
					width: 180px;
					padding: 10px;
					font-size: 12px;
					color: white;
					background: rgba(0 , 0, 0, 0.8);
					border-radius: 4px;
					display: none;
				}
				.TipsMsg_1{
					left: 315px;
				}
				.TipsMsg_2{
					right: 50px;
				}
			}
			.slide-box{
				background: white;
				.slide_box{
					width: 100%;
					height: 517px;
					padding: 120px 20px 20px;
					background: url(./images/bg2.png) no-repeat;
					background-position-x: center;
					.title{
						width: 283px;
						margin: auto;
						span{
							color: #ffad3d;
							font-size: 36px;
							margin: 15px 0 0 5px;
							font-style: oblique;
							font-weight: 700;
						}
					}
				}
			}
		
			
			.btn_box{
				.btn{
					width: 220px;
					height: 40px;
					line-height: 28px;
					background: #65290C;
					color: white;
					font-size: 16px;
					border-color: #65290C;
				}
			}
		}
	}
}
