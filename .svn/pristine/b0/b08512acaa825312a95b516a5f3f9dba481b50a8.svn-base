{ 
 "parserOptions": {
      "ecmaVersion": 8, 
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "globals": {
    "jQuery": true,
    "$": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "no-console": 0,
    "no-var": 0,
    "no-new": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": [0],
    "no-const-assign": "error",
    "prefer-template": 0,
    "object-shorthand": [0],
    "object-curly-newline": ["error", { "minProperties": 2 }],
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 0,
    "no-param-reassign": 0,
    "array-bracket-spacing": [2, "never"],
    "comma-dangle": [2, "ignore"], 
    "comma-spacing": [2, { "before": false, "after": true }],
    "computed-property-spacing": [2,"never"], 
    "semi": [2, "always"],
    "max-len": ["error", {
      "code": 10000,
      "tabWidth": 2,
      "comments": 200,
      "ignoreComments": false,
      "ignoreTrailingComments": false,
      "ignoreUrls": true
    }],
    "global-require": 0,
    "no-alert": 0,
    "no-restricted-syntax": [
      2,
      "LabeledStatement",
      "WithStatement"
    ],
    "no-extra-semi": 2,
    "use-isnan": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "quotes": ["error", "single"]
  }
}