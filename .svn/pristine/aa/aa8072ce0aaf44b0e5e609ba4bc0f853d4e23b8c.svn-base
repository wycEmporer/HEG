.prizeModal{
	.modal-dialog{
		width: 720px;
		height: 410px;
		position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%)!important;
		margin: 0px;
		.modal-content{
			width: 100%;
			height: 100%;
			text-align: center;
      color: #333333;
      border: none;
      background:url(./images/bg-left1.png) left top/184px 91px no-repeat, url(./images/bg-right1.png) right bottom/346px 94px no-repeat #f1f5f7;
      &.newPam{
        background:url(./images/bg-left.png) left top/79px 45px no-repeat, url(./images/bg-right.png) right bottom/115px 117px no-repeat #f1f5f7;
        .redeemBtn{
          background-color: #f23849;
        }
      }
			.modal-header{
				padding: 0px;
				position: relative;
        border: none;
        padding-top: 26px;
        // margin-bottom: 15px;
				.close{
					width: 29px;
					height: 29px;
					background: url('./images/guanbi@2x.png') no-repeat;
					position: absolute;
					right: 10px;
					top: 10px;
        }
        .validity{
          line-height: 30px;
        }
			}	
    }
    .modal-body{
      // padding:30px 0 0 0;
      height: 198px;
      position: relative;
    }
    .imgBox{
      display: flex;
      justify-content: center;
      margin-bottom: 20px;
    }
    .couponBan{
      border-radius: 4px;
      overflow: hidden;
      & +.couponBan{
        margin-left: 20px;
      }
      .couponImg{
        display: block;
        width: 280px;
        height: 170px;
      }
      .btnCopy{
        display: flex;
        justify-content: center;
        align-items: center;
        position: absolute;
        width:120px;
        height: 36px;
        bottom:0;
        right:0;
        border-radius: 12px 0 0 0;
        background-color: #fff;
        .coupon-value{
          position: relative;
          cursor: pointer;
          font-size:20px;
          padding: 0 6px;
          border: 1px dashed  #e51e1e;
          img{
            position: absolute;
            width:21px;
            right:-16px;
            top:-8px;;
          }
        }
      }
    }
    .redeemBtn{
      display: block;
      height: 50px;
      font-size: 20px;
      line-height: 50px;
      color:#fff;
      cursor: pointer;
      background-color: #e51e1e;
    }
    .footNote{
      font-size: 14px;
      color:#999;
    }
    .TipsMsg{
      position: absolute;
      left:50%;
      transform: translateX(-50%);
      top: 145px;
      width: 180px;
      padding: 10px;
      font-size: 12px;
      color: white;
      background: rgba(0 , 0, 0, 0.8);
      border-radius: 4px;
      display: none;
    }
	}
}