html{
  font-size: 14px;
}
.container{
  max-width: 800px;
  padding: 0;
}
.header{
  background-color: #0b9d78;
  color:white;
  padding:20px;
  margin-bottom: 20px;
}
.marL10{
  margin-left: 10px;
}
.mm-3{
  width:30%;
  float: left;
  padding: 0 10px;
}
.c_group{
  margin-bottom: 15px;
}
.cont{
  padding: 0 10px;
}
a[data-toggle="collapse"]{
  display: inline-block;
  width: 100%;
}
.bankList{
  label{
    border: 1px solid #999;
    padding: 8px;
    border-radius:4px;
  }
  input[type=radio]{
    float: left;
  }
  .bankIcon{
    display: block;
    float: left;
    width: 110px;
    height: 20px;
    margin-left: 10px;
    background-repeat: no-repeat;
    background-position: left center;
  }
  .hdfc{
    background-image: url('./imgs/HDFC.png');
  }
  .sbi{
    background-image: url('./imgs/sbi.png');
  }
  .kotak{
    background-image: url('./imgs/kotak.png');
  }
  .icici{
    background-image: url('./imgs/icici.png');
  }
  .axis{
    background-image: url('./imgs/AXIS.png');
  }
  .yes{
    background-image: url('./imgs/yes.png');
  }

  // wallet
  .freecharge{
    background-image: url('./imgs/freecharge.png');
  }
  .mobikwik{
    background-image: url('./imgs/mobikwik.png');
  }
  .ola{
    background-image: url('./imgs/ola.png');
  }
  .jiomoney{
    background-image: url('./imgs/jiomoney.png');
  }
  .oxigen{
    background-image: url('./imgs/oxigen.png');
  }
  .airtelmoney{
    background-image: url('./imgs/airtelmoney.png');
  }
}

.otherList{
  select{
    width: 100%;
    height: 38px;
    padding: 0 10px;
  }
}
.upi{
  padding: 20px 0 30px;
  input{
    height: 36px;
    width: 300px;
    padding-left: 10px;
  }
}