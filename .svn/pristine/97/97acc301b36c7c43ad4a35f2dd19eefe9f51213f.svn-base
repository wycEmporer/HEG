
@themeColor: #2ca5ea;
.body{
	background: #f5f5f5;
}
.content{
	.container{
		.wrap{
			margin-top: 30px;
			.anniverBox{
				width: 100%;
				height: 560px;
				background: url('./images/ann_bg.png') no-repeat;
				.ann_left{
					position: relative;
					width: 440px;
					height: 500px;
					margin: 60px 0px 0px 187px;
					.anniver{
						margin: auto;
						width: 380px;
						height: 370px;
						background: url('./images/ann.png') no-repeat;
						.num_box{
							width: 250px;
							height: 35px;
							position: absolute;
							left: 79px;
							top: 200px;
							.num{
								width: 70px;
								height: 38px;
								background: url('./images/prize.jpg') top center repeat-y;
								float: left;
								&:nth-child(2){
									margin: 0 20px 0 17px;
								}
							}
						}
					}
					.Handle{
						position: absolute;
						right: 28px;
						top: 160px;
						width: 48px;
						height: 187px;
						background: url('./images/down.png') no-repeat;
					}
					.up{
						position: absolute;
						right:28px;
						top: 42px;
						width: 48px;
						height: 187px;
						background: url('./images/up.png') no-repeat;
					}
					.lotteryNum{
						text-align: center;
						font-size: 16px;
						color: white;
						height: 15px;
						line-height: 15px;
						margin: 10px 0px 5px;
						.getChange{
							cursor: pointer;
						}
					}
					.startBtn{
						display: inline-block;
						width: 440px;
						height: 70px;
						background: url('./images/btn-on.png') no-repeat;
						margin-top: 5px;
						&:active{
							background: url('./images/btn-off.png') no-repeat;
						}
					}
				}
				.ann_right{
					width: 420px;
					margin:180px 20px 0px 0px; 
					color: white;
					.Prize{
						width: 420px;
						height: 360px;
						
						border-radius: 13px;
						margin-bottom: 20px;	
						.loginBtn{
							width: 100%;
							height: 100%;
							padding: 10px 20px;
							background:url('./images/noprize.png') no-repeat;
							.title{
								font-size: 14px;
								margin-top: 15px
							}
							.login_btn{
								width: 140px;
								height: 36px;
								border: 1px solid #FFFFFF;
								border-radius: 18px;
								text-align: center;
								line-height: 36px;
								font-size: 18px;
								margin:105px auto 20px;
								a{
									display: inline-block;
									color: white;
									width: 100%;
									height: 100%;
								}
							}
							.info{
								text-align: center;
								font-size: 12px;
							}
						}
						.noPrize{
							width: 100%;
							height: 100%;
							background:url('./images/noprize.png') no-repeat;
							text-align: center;
							padding-top: 115px;
							span{
								font-size: 20px;
								color: white;
							}
						}
						.havePrize{
							width: 100%;
							height: 100%;
							padding: 5px 0px 5px 20px;
							background: #6F0F37;
							border-radius: 13px;
							.prizeBox{
								width: 400px;
								height: 100%;
								overflow: auto;	
							
								&::-webkit-scrollbar{
									width: 3px;
								}
								&::-webkit-scrollbar-thumb{
									border-radius: 10px;
									background-color: #555;
								}
								ul{
									li{
										width: 380px;
										border-bottom: 1px solid white;
										position: relative;
										padding:5px 0px 5px 90px;
										min-height: 95px;
										cursor: pointer;
										.img{
											position: absolute;
											left: 0;
											top: 7px;
											width: 80px;
											height: 80px;
											img{
												width: 100%;
												height: 100%;
											}
										}
										.prize{
											line-height: 21px;
											span{
												&:nth-child(1){
													font-size: 14px;
												}
											}
										}
										.info{
											
											p{
												line-height: 15px;
    										font-size: 12px;
											}
											.prizeInfo{
												position: absolute;
												bottom: 10px;
												right: 0px;
												width: 90px;
												height: 26px;
												text-align: center;
												line-height: 26px;
												font-size: 16px;
												border-radius: 2px;
											}
											.Check{	
												border: 1px solid #FFA000;
												color: #FC9525;
											}
											.Redeem{
												background: #FC9525;
												color: white;
											}
										}
										.noBtn{
											padding-right: 0px;
										}
									}
								}
							}
							
						}
					}
					
				}
			}
			.anniverDetails{
				margin: 50px 0px;	
				color: #333333;
				font-size: 14px;
				.details{
					width: 100%;
					height: 60px;
					line-height: 60px;
					background: #FAFAFA;
					font-size: 20px;
					color: #0B9D77;
					padding: 0px 15px;
				}
				.infoItem{
					margin-top: 20px;
				}
				.title{
					height: 40px;
					line-height: 60px;
					font-size: 16px;
				}
				ul{
					li{
						line-height: 28px;
						font-size: 14px;
					}
				}
				.what,.prizeUl{
					padding-left:15px; 
					li{
						list-style: disc;
					}
				}
				.prizeTC{
					border: 1px solid #CDCDCD;
					border-top: none;
					margin-top: 10px;
					li{
						p{
							width: 100%;
							height: 32px;
							border: 1px solid #CDCDCD;
							border-top: 2px solid #CDCDCD;
							border-bottom: 0px;
							padding: 0px 20px;
							.icon{
								display: inline-block;
								margin-top: 9px;
								width: 23px;
								height: 14px;
								background: url('./images/unfold.png') no-repeat;
							}
							.curr{
								background: url('./images/fold.png') no-repeat;
							}
						}
						div{
							overflow: hidden;
							background: #F8FBFB;
							height: 0;
							padding: 0px 0px;
							ul{
								padding: 0 40px;
								li{
									list-style: disc;
										ol{
											padding-left: 15px;
											li{
												list-style: decimal;
											}
										}
									}
								}
							p{
								padding: 0px 25px;
								border: none;
							}
						}
					}
				}
			}
		}
	}
}
.noPrizeModal{
	.modal-dialog{
		width: 680px;
		height: 435px;
		position: absolute;
    left: 50%;
    top: 50%;
		transform: translate(-50%,-50%)!important;
		margin: 0px;
		.modal-content{
			width: 100%;
			height: 100%;
			text-align: center;
			color: #333333;
			.modal-header{
				height: 63px;
				padding: 0px;
				position: relative;
				border: none;
				.close{
					width: 29px;
					height: 29px;
					background: url('./images/close.png') no-repeat;
					position: absolute;
					right: 10px;
					top: 10px;
				}
				.title{
					font-size: 24px;
					margin-top: 30px;
				}
			}
			.modal-body{
				padding:30px 0 0 0;
				height: 260px;
				.prize_img{
					margin: auto;
					width: 160px;
					height: 160px;
					background: url('./images/cry.png') no-repeat;
				}
				.msg{
					height: 70px;
					line-height: 70px;
					font-size: 14px;
				}
			}
			.modal-footer{
				text-align: center;
				border: none;
				padding: 0px;
				a{
					width: 320px;
					height: 52px;
					background: #CC2232;
					border-radius: 4px;
					color: white;
					border: none;
					line-height: 40px;
					font-size: 16px;
					&:active{
						border: none;
					}
				}
			}
		}
	}
}
