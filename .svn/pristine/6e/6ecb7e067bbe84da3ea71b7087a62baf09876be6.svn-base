.searchNav{
  min-height: 100px;
  padding:8px;
  // background-image: linear-gradient(to left, #9f7782, #c8aeac,#9f7782);
  .tripType{
    color:#fff;
    padding:4px 0;
    .item{
      margin-right:10px;
      input{
        vertical-align: -10%;
      }
    }
  }
  .seaInput{
    input, select, button{
      height:40px;
    }
    .item{
      width:140px;
      margin-left:10px;
      &:first-child{
        margin-left: 0;
      }
      &.city{
        width:200px;
      }
    }
  }

  input.D_date{
    background: #fff;
  }
  input.R_date.active{
    background: #fff;
  }
  .SelectWrap{
    // display: block;
    top:40px;
    left:-50%;
    padding:16px;
    width: 270px;
    .fore{
      width:78px;
      float:left;
    }
    select{
      height:21px;
      display: block;
      width:50px;
    }
  }
}
.selectItem{
  position: relative;
  a.trallerpop{
    display: block;
    width:100%;
    padding-right: 30px;
    background: #fff url('./images/d-arrow.png') no-repeat 94% 50%;
    height: 40px;
    padding:11px 14px;
    border-radius: 4px;
    font-size: 14px;
    color:rgb(85,85,85);
    border:1px solid rgb(204, 204, 204);
  }
}
.cabinItem{
  select.form-control{
    appearance:none;
    background: #fff url('./images/d-arrow.png') no-repeat 94% 50%;
  }
}
.autoComplete{
  &:empty{
    display: none;
  }
  max-height: 292px;
  overflow-y: auto;
  background-color:#fff;
  border:1px solid #ccc;
  box-shadow: 0 3px 3px #aaa;
  z-index: 10;
  .item{
    height:32px;
    line-height: 32px;
    padding:0 5px;
    cursor:pointer;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
   &.active{
      background-color:#36c;
    }
  }
}
