<script id="tripsListTpl" type="text/template">
  <div class="tripsTitle clearfix" id='tripsTitle'>
    <ul>
      <li class='Tobepaid'><a href="javascript:;"><i class='icon icon-num'>{{= model.ToBePaidNum}}</i><span>To be paid</span></a></li>
      <li class='Upcoming'><a href="javascript:;"><span>Upcoming</span></a></li>
      <li class='Completed'><a href="javascript:;"><span>Completed</span></a></li>
      <li class='Canceled'><a href="javascript:;"><span>Canceled</span></a></li>
    </ul>
  </div>
  <div class="list-wrap content">
    <form class="query-form clearfix">
      <div class="group">
        <!--<label for="name">Name</label>-->
        <div class="trip-query">
          <input type="text" name="travellers" placeholder='name'>
        </div>
      </div>
      <div class="group">
        <!--<label for="tripID">Order ID</label>-->
        <div class="trip-query">
          <input type="text" name="tripid" placeholder='Order ID / Trip ID'>
        </div>
      </div>
      <div class="group">
        <!--<label for="departure">Departure</label>-->
        <div class="trip-query">
          <input type="text" name="depart" placeholder='Departure'>
        </div>
      </div>
      <div class="group">
        <!--<label for="return">Arrival</label>-->
        <div class="trip-query">
          <input type="text" name="arrive" placeholder='Destination'>
        </div>
      </div>
      <!--<div class="group">
        <label for="state">Status</label>
        <div class="trip-query select">
          <select name="state">
            <option value="">Status</option>
            {{ _.each(model.stateArr,function(val,i){ }}
            <option value="{{=val}}">{{=val}}</option>
            {{ }) }}
          </select>
        </div>
      </div>-->
      <div class="group">
        <button type="button" id="orderFilterBtn" class="btn btn-warning">Search</button>
      </div>
    </form>
    <div class='tab-info'>
      {{ if(model.stateArr[0] == "To be paid"){ }}
      <p><span>Results of range:</span> create time within 30 min</p>
      {{ }else if(model.stateArr[0] == "Upcoming" && !model.isLogin) { }}
      <p><span>Results of range:</span> create time within 1 year</p>
      {{ }else if(model.stateArr[0] == "Upcoming" && model.isLogin) { }}
      <p><span>Results of range:</span> all the time</p>
      {{ }else if(model.stateArr[0] == "Completed" && !model.isLogin) { }}
      <p><span>Results of range:</span> create time within 1 year</p>
      {{ }else if(model.stateArr[0] == "Completed" && model.isLogin) { }}
      <p><span>Results of range:</span> all the time</p>
      {{ }else if(model.stateArr[0] == "Canceled") { }}
      <p><span>Results of range:</span> create time within 7 days</p>
      {{ } }}
    </div>
    {{ if(model.list.length == 0){ }}
      {{ if(model.code == 504){ }}
      <div class='noOrderList loadTimeOut'></div>
      {{ }else{ }}
      <div class='noOrderList'></div>
      {{ } }}
    {{ }else{ }}
    <div class="orders clearfix">
      {{ _.each(model.list,function(val,i){}}
      <a href="/account/?src=trips/{{=val.flightOrderIdString}}&token={{=model.token}}" class="order-item {{=val.bookStatus.replace(/\s/g,'')}}" tripid="{{=val.flightItineraryIdString}}"
        orderid="{{=val.flightOrderIdString}}" travellers="{{=val.travellers}}" departure="{{=val.arriveCity}}" arrive="{{=val.departCity}}" state="{{=val.bookStatus}}">
        <ul class="clearfix">
          <li class="fore0">
            <div class="card">
              <div class="c-month">{{=val.month}}</div>
              <div class="c-data">
                <span class="day">{{=val.date}}</span>
                <span class="week">{{=val.week}}</span>
              </div>
            </div>
          </li>
          <li class="fore1">
            <div class="f-route">
              <div class="citys">
                <span class="dc">{{=val.departCity}}</span>
                <span class="icon" style="width:30px;font-size: 16px;font-weight: bold">&rarr;</span>
                <span class="ac">{{=val.arriveCity}}</span>
              </div>
              <p class="passengers">
                {{=val.travellers}}
              </p>
            </div>
          </li>
          <li class="fore2">
            <div class="other-info">
              <div class="status-btn">
                {{=val.bookStatus}}
              </div>
              {{if(val.bookStatus == 'To be paid') { }}
                <div data-id='{{=val.flightOrderIdString}}' data-index = '1'  class="cancel-btn">Cancel</div>
              {{ } }}
              
              <p class="info">
                Trip ID:
                <span class="id">{{=val.flightItineraryIdString}}</span>
              </p>
              <p class="info">
                Booked on:
                <span class="date">{{=val.bookOnString}}</span>
              </p>
            </div>
          </li>
        </ul>
      </a>
      {{ }) }}
    </div>
    {{ } }}
    {{if(model.list.length != 0){ }} 
    <div id="paginationContral" class='pagination-contral pull-right'></div>
    {{ }else{ }}
    <div id="paginationContral" class='pagination-contral hide pull-right'></div>
    {{ } }}
  </div>
</script>
