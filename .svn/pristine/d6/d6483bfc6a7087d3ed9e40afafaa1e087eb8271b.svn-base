<script id="vmcarouselListTpl" type="text/template">
	<ul class="vmcarousel-centered-infitine vmc-centered">
	{{ _.each(myModel, function(model, key){ }}
		<li><a href="/shake"><img src="{{= model}}" alt="" width="220"></a></li>
	{{ }) }}
	</ul>
</script>


