  .passInfo{
    .fl_info{
      padding:10px 5px 10px 110px;
      position: relative; 
      ul{
        li{
          margin-right: 40px;
          &:last-child{
            margin: 0;
          }
          &:nth-child(1){
            position: absolute;
            left: 10px;
            top: 8px;
            font-size: 16px;
            width: 100px;
            margin: 0;
            line-height: 30px;
          }
          p{
            font-size: 12px;
            color: #999;
            line-height: 20px;
            &:nth-child(1){
              font-size: 16px;
              color: #333;
              line-height: 30px;
            }
          }
        }
      }
    }
  }
  .conInfo{
    margin-bottom: 20px;
    .fl_info{
      ul{
        li{
          margin-right: 50px;
          p{
            height: 30px;
            line-height: 30px;
            font-size: 14px;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
            span{
              color: #999;
            }
          }
          &:nth-child(1){
            p{
              width: 250px;
            }
          }
          &:nth-child(2){
