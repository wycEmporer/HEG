.firstAnn {
  position: fixed;
  right: 0;
  top: 200px;
  z-index: 10;
}
.conLeft{
  width:906px;
  margin-bottom: 20px;
}

.sec{
  background-color: #f9fbfb;
  margin-bottom: 10px;
  &.closed{
    .viewDetail{
      display: block;
    }
    .content{
      display: none;
    }
  }
  &.open{
    .viewEdit{
      // display: none;
    }
  }
  &>.nav{
    height:46px; 
    padding:10px;
    background-color: @primary-light;
    border-radius: 4px;
    .num{
      float:left;
      width:26px;
      height:26px;
      color:#fff;
      line-height: 26px;
      background-color: @brand-primary;
      text-align: center;
      margin-right: 10px;
      border-radius: 15px;
    }
    .til{ 
      float:left;
      font-size: 18px;
      color:@brand-primary;
      margin-top: 5px;
    }
  }
  .fInfo{
    padding: 20px 40px;
    .halfCircleTop{
      position: absolute;
      width:20px;
      height:10px;
      border-radius: 0 0 50px 50px;
      border:1px solid #ccc;
      border-top-color: #fff;
      top:-1px;
      left:100px;
    }
    .halfCircleBot{
      position: absolute;
      width:20px;
      height:10px;
      border-radius: 50px 50px 0 0;
      border:1px solid #ccc;
      border-bottom-color: #fff;
      bottom:-1px;
      left:100px;
    }
    .fInfoI{
      position: relative;
      border:1px solid #ccc;
      border-radius: 4px;
      background-color: #fff;
    }
    .title{
      padding-bottom: 20px;
      span{
        float:left;
      }
      .city{
        font-weight: bold;
      }
    }

    ul.fLine{
      padding: 30px 0;
      li{
        float:left;
        padding-left: 20px;
        span,strong{
          display: block;
        }
        span{
          font-size: 14px;
          color:@gray-darker;
        }
        &.vendor{
          width:20%
        }
        &.start{
          width:24%;
        }
        &.through{
          width:30%;
          padding: 10px 0 0 0;
          .line{
            width:26%;
            border-bottom: 1px dashed #ccc;
            padding-top: 16px;
          }
          .dur{
            width:48%;
            text-align: center;
          }
        }
        &.end{
          width:24%;
        }
       
      }
    }
    .bagInfo{
      padding:10px;
      border-top: 1px solid @gray-lighter;
    }
    .layover{
      padding: 10px 0;
      background-color: #ffefd8;
      .line{
        border-bottom: 1px solid @brand-warning;
        padding-top: 12px;
      }
      .layoverTime{
        text-align: center;
      }
    }
  }      
}
.Tra_public{
  padding: 10px 40px;
  background-color: #ffefd8;
}
.passForm{
  .note{
    padding: 10px 40px;
  }
  .listWrap{
    .title{
      padding-top: 10px;
      // margin-top: 20px;
      text-align: center;
      font-size: 18px;
    }
    .passengerInfo{
      margin:0 40px 10px;
      background-color: @gray-lighter;
      border:1px solid @gray-light;
      border-radius: 4px;
      padding: 6px 0;
      position: relative;
      .ic{
        background-image: url(./imgs/perIcon.png);
        background-repeat: no-repeat;
        background-position: center;
        background-size: 40px 40px;
        height: 40px;
        width: 40px;
        left: 44px;
        top:50%;
        transform:translateY(-50%);
        position: absolute;
      }
    }

    .formItem{
      margin: 5px 0;

      .formItemTil{
        font-size: 14px;
        line-height: 20px;
        // height: 20px;
      }
    }
  }
  .travellerHisWrap{
    position: relative;
    height: 42px;
    overflow: hidden;
    &.collapseWrap{
      height: auto;
    }
    .nameItem{
      float:left;
      margin-right: 40px;
      width: 150px;
      label{
        white-space: nowrap;
        text-overflow: ellipsis;
        overflow: hidden;
      }
    }
    .traMore{
      position: absolute;
      right: 60px;
      top: 10px;
    }
  }
  .inputBox{
    // background-color: #fff;
    // height:50px;
    input, select{
      border:1px solid #ccc;
    }
  }
  input[type=checkbox], input[type=radio]{
    margin-top:3px;
  }
  .form-con{
    width:100%;
    height:40px;
    background-color: #fff;
    // border:none;
    padding:10px 8px;
    &[readonly]{
      background-color: #eee;
    }
  }
  .silverOtpBtn{
    color:#fff;
    background-color: @brand-warning;
    user-select:none;
    border:none !important;
    &[disabled]{
      cursor:not-allowed;
      opacity: .65;
      box-shadow: none;
    }
  }
  .errorInput{
    border:1px solid #f65858 !important;
  }
  .form-gro{
    margin-bottom: 5px;
  }
  .con-label{
    font-size: 14px;
    line-height: 39px;
    font-weight: normal;
  }
  .wrapMargin{
    margin: 20px 40px;
    .btnmy{
      width:200px;
    }
  }
  #insureWrap{
    .insHead{
      height: 46px;
      padding: 10px;
      background-color: #e1f1ee;
      border-radius: 4px;
      position: relative;
      
      .num{
        float: left;
        width: 20px;
        height: 26px;
        margin-right: 10px;
        background:url('./imgs/ins-icon.png') no-repeat;
      }
      .til{
        float: left;
        font-size: 18px;
        color: #0b9d78;
        margin-top: 5px;
      }
     
      .img_icon{
        display: inline-block;
        width: 110px;
        height: 16px;
        margin-top: 3px;
        background: url('./imgs/insure.png') no-repeat;
        background-size: 100%;
        margin-right: 35px;
      }
      
    }
  }
  .insureWrap{
    .title{
      height: 26px;
      line-height: 29px;
      font-size: 17px;
      color: #ff4c1c;
      padding-left: 5px;
      margin-bottom: 10px;
    }
    .benWrap{
      transition: all .3s ease-in;
      overflow: hidden;
      height: 0;
      width: 806px;
      padding-top: 10px;
    }
    .b-item{
      float:left;
      width: 375px;
      height:40px;
      padding:5px 10px;
      text-align: center;
      border-radius: 3px;
      background: white;
      margin-right: 2px;
      margin-bottom: 10px;
      &:nth-child(odd){
        margin-right: 52px;
      }
      .imgWrap{
        float: left;
        height: 30px;
        height:30px;
        margin: 0 10px;
      }
      p{
        float: left;
        text-align: left;
        width: 125px;
        height: 30px;
        line-height: 31px; 
       }
       .tips{
        width: 160px;
        margin:0px 10px;
        padding-right: 10px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
      }
    }
    .insureCount{
      width: 200px;
     .count{
        width: 200px;
        height:30px;
        line-height: 30px;
        background-color: #e1f1ee;
        margin-top: 8px;
     }
    }
    
    .loadMore{
      // padding: 10px 0;
      text-align: center;
      position: relative;
      margin-right: 10px;
      width: 600px;
      .trip{
        float: left;
        width: 290px;
        height: 150px;
        background: #eff7f8;
        padding: 15px;
        &:first-child{
          margin-right: 20px;
        }
        img{
          width: 50px;
          height: 50px;
        }
        p{
          &:nth-child(2){
            font-size: 14px;
            color: #333;
            line-height: 24px;
            margin-top: 15px;
          }
          &:nth-child(3){
            font-size: 20px;
            color: #999;
            span{
              color: #333;
              font-weight: 700;
            }
          }
        }
      }
     
      .showMore{
        display: inline-block;
        width: 600px;
        height: 30px;
        line-height: 30px;
        background-color: #ffffff;
        border-radius: 2px;
        border: solid 1px #eeeeee;
        margin-top: 10px;
        cursor: pointer;
        .moreNum{
          span{
            font-size: 16px;
            position: relative;
            &::before{
              content: '+';
              position: absolute;
              left: -12px;
              top: -6px;
              font-size: 16px;
            }
          }
        }
        .text{
          color: #333;
          font-size: 16px;
        }
        .btn_showMore{
          position: relative;
          display: inline-block;
          height: 10px;
          width: 20px;
          .drow{
            position: absolute;
            left: 50%;
            bottom: 3px;
            transform: translateX(-50%);
            width: 0;
            height: 0;
            border-top: 6px solid transparent;
            border-left: 6px solid transparent;
            border-right: 6px solid transparent;
            border-bottom: 6px solid #0b9d78;
            cursor: pointer;
          }
          .down{
            position: absolute;
            left: 50%;
            top: 3px;
            transform: translateX(-50%);
            width: 0;
            height: 0;
            border-top: 6px solid #0b9d78;
            border-left: 6px solid transparent;
            border-right: 6px solid transparent;
            border-bottom: 6px solid transparent;
    
          }
        }
      }
      
    }
    .sec-trip{
      width: 200px;
      height: 190px;
      background-color: #ffe8dd;
      box-shadow: 0px 2px 8px 0px 
        rgba(0, 0, 0, 0.3);
      border-radius: 4px;
      border: solid 1px #fbe4e0;
      display: table-cell;
      vertical-align: middle;
      cursor: pointer;
      position: relative;
      &::before{
        content: '';
        position: absolute;
        left: 30px;
        top: 2px;
        width: 164px;
        height: 186px;
        background: url(./imgs/san-pc.png) no-repeat;
      }
      p{
        font-size: 18px;
        color: #ff4c1c;
        &:nth-child(1){
          padding-left: 20px;
          i{
            display: inline-block;
            width: 20px;
            height: 20px;
            background: url(./imgs/noslc.png) no-repeat;
            margin-right: 10px;
            // cursor: pointer;
            &.active{
              background: url(./imgs/sle.png) no-repeat;
            }
          }
        }
        &:nth-child(2){
          margin-left: 50px;
          span{
            font-size: 22px;
          }
        }
      }
    }
    .form-gro.row{
      .col-sm-8{
        margin-right: 60px;
      }
    }
  }
}

.section3{
  .form-con{
    width:100%;
    height:36px;
    background-color: #fff;
    padding:10px;
    &[readonly]{
