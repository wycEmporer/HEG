            &:nth-child(1){
              border-top: 1px solid #ccc;
            }
            &:hover{
              background: #0b9d78;
              color: white;
            }
          }
        }
        .noList{
          width: 176px;
          height: 90px;
          padding: 10px;
          .icon{
            display: block;
            width: 100%;
            height: 46px;
            background: url('./imgs/wu.png') no-repeat;
            background-position: center;
          }
          p{
            height: 24px;
            line-height: 30px;
            text-align: center;
          }
        }
      }
    }
    
    .goldbackPop{
      display: inline-block;
      // width: 130px;
      height: 25px;
      line-height: 25px;
      background: url('./imgs/EarnIcon.png') no-repeat;
      // position: relative;
      padding-left: 40px;
      color: red;
      .earnPrice{
        display: inline-block;
        padding: 0 5px;
        border: 1px solid #d72227;
        height: 24px;
        // position: absolute;
        // left: 45px;
        // top: 3px;
      }
    }
    .couponInfo{
      font-size: 12px;
      line-height: 14px;
      color: #666666;
    }
  }
  .apply{
    margin-top: -1px;
    width: 80px;
