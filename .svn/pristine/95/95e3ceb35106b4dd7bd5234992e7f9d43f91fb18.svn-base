      input{
        width: 100%;
        height: 100%;
      }
      .listIcon{
        position: absolute;
        right: 0;
        top: 0;
        width: 30px;
        height: 30px;
        background: url('./imgs/ture.png') no-repeat;
        background-position: center center;
      }
      .couponList{
        position: absolute;
        left: 0;
        top: 30px;
        border-left:1px solid #ccc; 
        border-right:1px solid #ccc; 
        border-radius: 4px;
        z-index: 2;
        background: white;
        max-height: 300px;
        overflow: auto;
        ul{
          li{
            width: 176px;
            height: 30px;
            border-bottom:1px solid #ccc; 
            padding: 0px 10px;
            line-height: 30px;
