  }
  .upFare{
    position: relative;
    height: 30px;
    background: #f9fbfb; 
    .upFareNote{
      display: none;
      position: absolute;
      background-color: #333;
      border-radius: 4px;
      top:-44px;
      left: -388px; 
      .arrow-t{
        position: absolute;
        right:-20px;
        top: 50%;
        border-top: 10px solid transparent;
        border-bottom: 10px solid transparent;
        border-right: 10px solid transparent;
        border-left: 10px solid #333;
      }
    }
    &:hover{
      .upFareNote{
        display: block;
