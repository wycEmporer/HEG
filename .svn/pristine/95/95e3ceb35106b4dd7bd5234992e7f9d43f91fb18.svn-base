  .flightInfo{
    .fl_info{
      ul{
        .color0{
          color: #0b9d78;
        }
        .color1{
          color: #ffad3d;
        }
        li{
          font-size: 14px;
          p{
            line-height: 30px;
          }
          &:nth-child(1){
            width: 80px;
            margin-top: 8px
          }
          &:nth-child(2){
            width: 100px;
            margin-top: 8px;
            .img{
              width: 30px;
              height: 30px;
              background: red;
            }
          }
          &:nth-child(3),
          &:nth-child(5){
            width: 165px;
            P{
              &:nth-child(1){
                color: #999;
              }
              &:nth-child(2){
                font-size: 16px;
              }
            }
          }
          &:nth-child(4){
            width: 100px;
            margin: 0px 60px 0px 60px;
            padding-top: 10px;
            p{
              text-align: center;
              line-height: 20px;
              &:nth-child(2){
