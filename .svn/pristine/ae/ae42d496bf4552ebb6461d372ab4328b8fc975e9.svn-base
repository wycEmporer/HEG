<script id="offerListTpl" type="text/template">
  {{_.each(model, function(val, key){ }}
  <li>
    <img src="{{=val.url}}" alt="" width=380 height="220">
    <div class="article">
      <div class="share clearfix">
        <p>{{=val.title}}</p>
        <!-- <a href="##" class="fb"></a>
        <a href="##" class="tt"></a> -->
      </div>
      <div class="showDetail clearfix">
        <div class="pull-left">
          <p>Validity:</p>
          <p class="grayDark">
            {{=val.period}}
          </p>
        </div>
        <div class="pull-right">
          <a href="{{=val.href}}" class="btn btn-warning">View Detail</a>
        </div>
      </div>
    </div>
  </li>
  {{})}}
</script>